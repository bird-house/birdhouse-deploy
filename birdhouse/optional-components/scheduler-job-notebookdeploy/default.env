export SCHEDULER_JOB_NOTEBOOKDEPLOY_IMAGE='${DOCKER_CLI_IMAGE}'

export BIRDHOUSE_AUTODEPLOY_NOTEBOOK_FREQUENCY="@hourly"

export SCHEDULER_JOB_NOTEBOOKDEPLOY_LOCAL_ENV_DIR_VOLUME='$([ -n "${BIRDHOUSE_LOCAL_ENV_REAL_DIR}" ] && printf " --volume %s:%s:ro" "${BIRDHOUSE_LOCAL_ENV_REAL_DIR}" "${BIRDHOUSE_LOCAL_ENV_REAL_DIR}" )'

DELAYED_EVAL="
  $DELAYED_EVAL
  SCHEDULER_JOB_NOTEBOOKDEPLOY_LOCAL_ENV_DIR_VOLUME
  SCHEDULER_JOB_NOTEBOOKDEPLOY_IMAGE
"

VARS="
  $VARS
  \$SCHEDULER_JOB_NOTEBOOKDEPLOY_IMAGE
"

OPTIONAL_VARS="
  $OPTIONAL_VARS
  \$SCHEDULER_JOB_NOTEBOOKDEPLOY_LOCAL_ENV_DIR_VOLUME
  \$BIRDHOUSE_AUTODEPLOY_NOTEBOOK_FREQUENCY
  \$AUTODEPLOY_NOTEBOOK_EXTRA_DOCKER_ARGS
"
